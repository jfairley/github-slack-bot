{
  "name": "github-slack-bot",
  "description": "Slack bot for your team which integrates with Github",
  "version": "0.0.0-dev",
  "author": "Jeff Fairley",
  "bugs": "https://github.com/jfairley/github-slack-bot/issues",
  "dependencies": {
    "@google-cloud/datastore": "4.0.0",
    "@google-cloud/debug-agent": "3.2.0",
    "@google-cloud/logging-winston": "0.11.1",
    "@octokit/rest": "16.28.1",
    "@octokit/webhooks": "5.1.2",
    "@slack/web-api": "5.0.1",
    "async": "3.0.1",
    "lodash": "4.17.10",
    "moment": "2.22.2",
    "tsscmp": "1.0.6",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "7.5.2",
    "@commitlint/config-conventional": "7.5.0",
    "@commitlint/prompt-cli": "7.5.0",
    "@octokit/webhooks-definitions": "1.1.2",
    "@types/express": "4.17.0",
    "@types/jest": "24.0.5",
    "@types/lodash": "4.14.110",
    "@types/nock": "10.0.3",
    "@types/randomstring": "1.1.6",
    "fixpack": "2.3.1",
    "husky": "1.3.1",
    "jest": "24.1.0",
    "jest-junit": "6.3.0",
    "lint-staged": "8.0.4",
    "nock": "9.6.1",
    "prettier": "1.16.4",
    "randomstring": "1.1.5",
    "semantic-release": "15.6.0",
    "ts-jest": "23.10.5",
    "tslint": "5.12.1",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.4.5"
  },
  "homepage": "https://github.com/jfairley/github-slack-bot#readme",
  "keywords": [
    "github",
    "slack",
    "slackbot"
  ],
  "license": "ISC",
  "main": "index.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jfairley/github-slack-bot.git"
  },
  "scripts": {
    "build": "tsc",
    "commit": "commit",
    "deploy:github": "tsc && gcloud functions deploy github --env-vars-file .env.yml --runtime=nodejs8 --trigger-http",
    "deploy:slack": "tsc && gcloud functions deploy slack --env-vars-file .env.yml --runtime=nodejs8 --trigger-http",
    "fixpack": "fixpack",
    "lint": "tslint --project tsconfig.json -c tslint.json",
    "predeploy": "npm run build",
    "pretest": "jest --clearCache",
    "release": "semantic-release",
    "start": "npx @google-cloud/functions-framework --target=slack",
    "test": "jest --coverage"
  }
}
